class NSine : CsWaveAudio.SynthBases.PeriodicSynthesisBase
{
	//example of basic sine
	public NSine(double freq, double amplitude)
		: base(freq, amplitude) { }
	protected override double WaveformFunction(int i)
	{
		 return Math.Sin(i * timeScale);
	}
}
 
//$$Main
WaveAudio input = new NSine(240,0.5).CreateWaveAudio(1.0);
result=input;
